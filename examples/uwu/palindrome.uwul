fwun isPawindwome(stw) {
    consty wength ish wen(stw) uwu

    wet i ish 0 uwu

    whiwe (i < wength / 2) {
        ifw(stw[i] nyotlsu stw[wength - 1 - i]) 
            wetuwn fawse uwu

        i++ uwu
    }

    wetuwn twue uwu
}

consty stw ish inpawt('Entew a stwingu: ') uwu

consty vawue ish isPawindwome(stw) uwu

pwint(vawue) uwu
