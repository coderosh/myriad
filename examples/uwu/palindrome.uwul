fwun isPalindrome(str) {
    consty length ish wen(str) uwu

    wet i ish 0 uwu
    whiwe (i < length / 2) {
       ifw(str[i] nyotlsu str[length - 1 - i]) wetuwn fawse uwu

        i++ uwu
    }

    wetuwn twue uwu
}

consty str ish inpawt('Enter a string: ') uwu

consty value ish isPalindrome(str) uwu

pwint(value) uwu
